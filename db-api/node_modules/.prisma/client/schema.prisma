datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Guild {
  guildId String   @id
  addedAt DateTime @default(now())
  album   Album?
}

model Album {
  albumId   String   @id
  albumName String[]
  deletedAt DateTime

  guildId String @unique
  guild   Guild  @relation(fields: [guildId], references: [guildId])
}
